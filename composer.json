{
  "name": "setbased/php-erd-concepts",
  "description": "Fixes (minor) issues in SQL code generated by ERD Concepts",
  "type": "library",
  "license": "MIT",
  "keyword": [
    "ERD",
    "Concepts",
    "fix"
  ],
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\ErdConcepts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\ErdConcepts\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
